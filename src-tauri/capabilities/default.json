{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "popup"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "global-shortcut:default",
    "positioner:default",
    "dialog:default",
    "dialog:allow-save",
    "dialog:allow-open",
    "dialog:allow-message",
    "autostart:default"
  ]
}